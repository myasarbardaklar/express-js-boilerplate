{
  "routes": [
    {
      "method": "GET",
      "path": "/users",
      "handler": "Admin/UserController.index",
      "authenticated": false
    },
    {
      "method": "GET",
      "path": "/users/user",
      "handler": "Admin/UserController.user",
      "authenticated": true
    },
    {
      "method": "GET",
      "path": "/users/moderator",
      "handler": "Admin/UserController.moderator",
      "authenticated": true,
      "permission": "moderator"
    },
    {
      "method": "GET",
      "path": "/users/admin",
      "handler": "Admin/UserController.admin",
      "authenticated": true,
      "permission": "admin"
    }
  ]
}
