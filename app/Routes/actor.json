{
  "routes": [
    {
      "method": "GET",
      "path": "/actors",
      "handler": "actor.find"
    },
    {
      "method": "POST",
      "path": "/actor",
      "handler": "actor.create",
      "authenticated": true,
      "permission": "moderator",
      "middleware": [
        {
          "path": "FileUpload",
          "method": "upload",
          "parameters": ["single", "photo"]
        }
      ]
    }
  ]
}
