{
  "routes": [
    {
      "method": "GET",
      "path": "/movies",
      "handler": "movie.find"
    },
    {
      "method": "POST",
      "path": "/movie",
      "handler": "movie.create",
      "authenticated": true,
      "permission": "moderator",
      "middleware": [
        {
          "path": "FileUpload",
          "method": "upload",
          "parameters": ["single", "poster"]
        }
      ]
    }
  ]
}
