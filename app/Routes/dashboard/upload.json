{
  "routes": [
    {
      "method": "GET",
      "path": "/dashboard/uploads",
      "handler": "dashboard/upload.index",
      "authenticated": true,
      "permission": "moderator"
    },
    {
      "method": "POST",
      "path": "/dashboard/upload",
      "handler": "dashboard/upload.create",
      "authenticated": true,
      "permission": "moderator",
      "middleware": [
        {
          "path": "FileUpload",
          "method": "upload",
          "parameters": ["multiple", "upload_images", 3]
        }
      ]
    }
  ]
}
